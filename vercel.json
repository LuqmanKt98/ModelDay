{
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/canvaskit/(.*)",
      "destination": "/canvaskit/$1"
    },
    {
      "source": "/icons/(.*)",
      "destination": "/icons/$1"
    },
    {
      "source": "/(.*\\.js)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.css)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.png)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.jpg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.jpeg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.gif)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.ico)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.svg)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.wasm)",
      "destination": "/$1"
    },
    {
      "source": "/(.*\\.json)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
